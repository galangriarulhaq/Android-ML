<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="RoundedButton" parent="Widget.MaterialComponents.Button">
        <item name="cornerRadius">8dp</item>
        <item name="backgroundTint">@color/md_theme_primary</item>
        <item name="android:textColor">@color/white</item>
        <item name="textAllCaps">false</item>
    </style>

    <style name="CustomToolbarTitleText" parent="TextAppearance.Widget.AppCompat.Toolbar.Title">
        <item name="android:textSize">24sp</item>
        <item name="android:fontFamily">@font/roboto</item>
        <item name="android:textColor">#FFFFFF</item>
    </style>


</resources>